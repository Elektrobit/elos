{
    "root": {
        "elos": {
            "UseEnv": false,
            "ConnectionLimit": 200,
            "LogFilter": "",
            "LogLevel": "DEBUG",
            "Port": 54321,
            "EventBlacklist": ".event.messageCode 2000 EQ",
            "authorizedProcesses": [
                ".process.uid 0 EQ .process.gid 0 EQ .process.exec '/bin/elosc' STRCMP AND",
                ".process.gid 200 EQ .process.exec '/bin/elosc' STRCMP AND",
                ".process.pid 1 EQ"
            ],
            "ClientInputs": {
                "PluginSearchPath": "/usr/local/lib/elos/client",
                "Plugins": {
                    "LocalTcpClient": {
                        "File": "client_dummy.so",
                        "Run": "always",
                        "Config": {
                            "Port": 54321,
                            "Interface": "127.0.0.1",
                            "EventBlacklist": ".event.messageCode 2000 EQ",
                            "authorizedProcesses": [
                                ".process.uid 0 EQ .process.gid 0 EQ .process.exec '/bin/elosc' STRCMP AND",
                                ".process.gid 200 EQ .process.exec '/bin/elosc' STRCMP AND",
                                ".process.pid 1 EQ"
                            ]
                        }
                    },
                    "PublicTcpClient": {
                        "File": "client_dummy.so",
                        "Run": "always",
                        "Config": {
                            "Port": 5422,
                            "Interface": "0.0.0.0",
                            "EventBlacklist": ".event.messageCode 2000 EQ",
                            "authorizedProcesses": []
                        }
                    }
                }
            },
            "EventLogging": {
                "PluginSearchPath": "/usr/lib/x86_64-linux-gnu/elos/backend",
                "Plugins": {
                    "Dummy": {
                        "File": "backend_dummy.so",
                        "Run": "always",
                        "Filters": [
                            "1 1 EQ"
                        ]
                    },
                    "JsonBackend": {
                        "File": "backend_json.so",
                        "Run": "always",
                        "Filters": [
                            "1 1 EQ"
                        ],
                        "Config": {
                            "StoragePath": ""
                        }
                    },
                    "DLT": {
                        "File": "backend_dlt_logger.so",
                        "Run": "always",
                        "Filter": [
                            ".messageCode 1000 GE"
                        ],
                        "Config": {
                            "Connection": "/tmp/dlt",
                            "EcuId": "ECU1",
                            "AppId": "ELOS"
                        }
                    }
                }
            },
            "StorageBackend": {
                "Json": {
                    "File": "/var/log/elos/elosd_event.log"
                }
            },
            "Scanner": {
                "Path": "/usr/local/lib/elos/scanner",
                "KmsgScanner": {
                    "KmsgFile": "/dev/kmsg"
                },
                "SyslogScanner": {
                    "SyslogPath": "/dev/log",
                    "MappingRules": {
                        "MessageCodes": {
                            "4000": ".event.source.appName 'ssh' STRCMP",
                            "2000": ".event.source.appName 'crinit' STRCMP",
                            "1000": ".event.source.appName 'login' STRCMP"
                        }
                    }
                },
                "Shmem": {
                    "ShmemFile": "scanner_shmem",
                    "ShmemCreate": true,
                    "ShmemLogEntries": 256,
                    "ShmemOffset": 0,
                    "SemFile": "scanner_shmem_sem",
                    "SemCreate": true
                }
            }
        }
    }
}
